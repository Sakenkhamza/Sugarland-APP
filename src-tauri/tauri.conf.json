{
    "$schema": "https://raw.githubusercontent.com/nicerdicer/tauri/dev/tooling/cli/schema.json",
    "productName": "Sugarland",
    "version": "0.1.0",
    "identifier": "com.sugarland",
    "build": {
        "frontendDist": "../dist",
        "devUrl": "http://localhost:1420",
        "beforeDevCommand": "npm run dev",
        "beforeBuildCommand": "npm run build"
    },
    "app": {
        "windows": [
            {
                "label": "main",
                "title": "Sugarland",
                "width": 1280,
                "height": 800,
                "minWidth": 1024,
                "minHeight": 600,
                "resizable": true,
                "fullscreen": false
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "macOS": {
            "minimumSystemVersion": "10.15"
        },
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ]
    }
}